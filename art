
function setup() {
  createCanvas(500, 500);
}
let DIAMOND=50
function draw() {
  background(250);
  for(var i=0;i<9;i++){
    for(var j=0;j<9;j++) {
     let topx = (i+1)*  DIAMOND
     let topy = j* DIAMOND
     let leftx = i* DIAMOND
     let lefty = (j+1)* DIAMOND
     let bottomx = (i+1)* DIAMOND
     let bottomy = (j+2)* DIAMOND
     let rightx = (i+2)* DIAMOND
     let righty = (j+1)* DIAMOND

quad (mouseX,mouseY,leftx, lefty, mouseX,mouseY,rightx,righty) 
    }
}
}
